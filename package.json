{
  "name": "product-color-matcher",
  "version": "1.0.0",
  "description": "An API to match similar color products",
  "main": "app/server/index.js",
  "scripts": {
    "migrate": "node-pg-migrate",
    "import": "run-func app/cmd/productImport run",
    "detect-colors": "run-func app/cmd/colorDetection run",
    "start": "node .",
    "lint": "jshint app",
    "clean": "",
    "pretest": "npm run lint && rimraf test-logs && mkdirp test-logs && cross-env NODE_ENV=test node-pg-migrate redo",
    "test": "cross-env NODE_ENV=test mocha tests --no-timeouts",
    "posttest": "cross-env NODE_ENV=test node-pg-migrate down 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msuret/product-color-matcher.git"
  },
  "keywords": [
    "color",
    "matching"
  ],
  "author": "Maxime Suret",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/msuret/product-color-matcher/issues"
  },
  "homepage": "https://github.com/msuret/product-color-matcher#readme",
  "dependencies": {
    "@google-cloud/vision": "0.15.2",
    "bluebird": "3.5.1",
    "bluestream": "6.1.1",
    "bulkstream": "1.0.1",
    "colour-proximity": "0.0.2",
    "config": "1.30.0",
    "csv-parser": "1.12.0",
    "express": "4.16.2",
    "js-yaml": "3.10.0",
    "lodash": "4.17.5",
    "morgan": "1.9.0",
    "node-pg-migrate": "2.24.0",
    "normalize-url": "2.0.1",
    "pg": "7.4.1",
    "pg-monitor": "0.9.0",
    "pg-promise": "8.1.1",
    "pg-query-stream": "1.1.1",
    "run-func": "1.0.2",
    "winston": "2.4.0",
    "winston-config": "0.5.1"
  },
  "devDependencies": {
    "cross-env": "5.1.3",
    "jshint": "2.9.5",
    "mkdirp": "0.5.1",
    "mocha": "5.0.3",
    "rimraf": "2.6.2",
    "sinon": "4.4.2"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "nocomma": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,
    "validthis": true,
    "esversion": 6
  }
}
